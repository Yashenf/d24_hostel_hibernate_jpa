<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="context" prefHeight="280.0" prefWidth="400.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yashen.d24_hostel.controller.user.UserMainFormController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="108.0" text="User Management" />
      <Button layoutX="278.0" layoutY="10.0" mnemonicParsing="false" onAction="#newUserBtnOnAction" prefHeight="25.0" prefWidth="108.0" text="New User">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="278.0" layoutY="44.0" mnemonicParsing="false" onAction="#updateUserOnAction" prefHeight="25.0" prefWidth="108.0" text="Update">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <TextField fx:id="searchTxt" layoutX="14.0" layoutY="44.0" prefHeight="25.0" prefWidth="251.0" promptText="Search here" />
      <TableView fx:id="userTbl" layoutX="17.0" layoutY="83.0" prefHeight="186.0" prefWidth="369.0">
        <columns>
          <TableColumn fx:id="noCol" prefWidth="75.0" text="No" />
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="mobileCol" prefWidth="75.0" text="Mobile" />
            <TableColumn fx:id="jobCol" prefWidth="75.0" text="Job position" />
            <TableColumn fx:id="optionCol" prefWidth="66.0" text="Option" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
